        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> f_MRP2 </ci>
              <ci> PRAEHC_Vmax </ci>
              <ci> Vli </ci>
